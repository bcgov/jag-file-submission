  "private": true,
  "dependencies": {
    "@bcgov/bootstrap-theme": "github:bcgov/bootstrap-theme",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "node-sass": "^6.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
