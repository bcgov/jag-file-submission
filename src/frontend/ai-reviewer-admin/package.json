    "react-dropzone": "^11.3.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "shared-components": "https://github.com/bcgov/react-shared-components/releases/download/v1.2.3/shared-components-1.2.3.tgz",
